export ZSH=~/.oh-my-zsh

ZSH_THEME="powerline"

plugins=(git bundler osx ubuntu rake ruby autojump tmux mvn virtualenv dircolors mycli fzf zsh-autosuggestions zsh-syntax-highlighting)

source $ZSH/oh-my-zsh.sh

export LANG=en_US.UTF-8
export PATH=$PATH:/usr/local/sbin:$HOME/bin

alias ll="ls -la"
alias ll='ls -alF'
alias lt='ls -lt'
alias ld='ls -ad'
alias la='ls -A'
alias l='ls -CF'

