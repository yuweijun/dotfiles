#!/usr/bin/env zsh

export ZSH=~/.oh-my-zsh

ZSH_THEME="powerlevel9k.clean"

plugins=(aliases
    alias-finder
    autojump
    dircolors
    fzf
    git
    kubectl
    mycli
    nvm
    systemd
    tmux
    virtualenv
    z
    zsh-autosuggestions
    zsh-colors
    zsh-syntax-highlighting
    zsh_reload
    env
)

source $ZSH/oh-my-zsh.sh

